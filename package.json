{
  "name": "ts-print",
  "description": "Biblioteca útil para debug em node, com cores e datas formatadas, se propõe a ser intuitiva.",
  "version": "1.1.2",
  "license": "MIT",
  "author": "Marcus Yoda <marcus@acelera.cloud>",
  "repository": "https://github.com/aceleracloud/ts-print.git",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "files": [
    "lib",
    "package.json"
  ],
  "scripts": {
    "build": "rollup -c",
    "ci": "yarn --frozen-lock",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "deploy": "yarn build && yarn publish",
    "format:audit": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:audit": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "mocha -r ts-node/register tests/*.test.ts"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.5.6",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "chai": "^4.3.4",
    "concurrently": "^8.2.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "rollup": "^3.2.3",
    "rollup-plugin-typescript2": "^0.35.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {}
}
