{
  "name": "ts-print",
  "description": "Biblioteca útil para debug em node, com cores e datas formatadas, se propõe a ser intuitiva.",
  "version": "2.0.0",
  "license": "MIT",
  "author": "Marcus Yoda <marcus@acelera.cloud>",
  "repository": "https://github.com/aceleracloud/ts-print.git",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "files": [
    "lib",
    "package.json"
  ],
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "ci": "yarn --frozen-lock",
    "deploy": "yarn build && yarn publish",
    "format:audit": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:audit": "eslint .",
    "lint:fix": "eslint --fix .",
    "ts:audit": "tsc --noEmit",
    "src:prepare": "yarn format:fix && yarn lint:fix && yarn ts:audit",
    "test": "mocha -r ts-node/register tests/*.test.ts"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@types/node": "^20.5.6",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^14.0.1",
    "prettier": "^2.8.8",
    "rollup": "^3.2.3",
    "rollup-plugin-typescript2": "^0.35.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {}
}
